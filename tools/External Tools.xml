<toolSet name="External Tools">
  <tool name="dotnet" description="dotnet" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="C:\Program Files\dotnet\dotnet.exe" />
      <option name="PARAMETERS" value="watch run" />
      <option name="WORKING_DIRECTORY" value="C:/projects/care-more/server/CareMore" />
    </exec>
  </tool>
  <tool name="prettier" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="prettier" />
      <option name="PARAMETERS" value="--write $FilePathRelativeToProjectRoot$" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
</toolSet>